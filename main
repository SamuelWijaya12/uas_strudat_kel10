#include<stdio.h>
#include<stdlib.h>
#include<conio.h>
#include<string.h>

#define max 10

struct node{
	int noantrian;
	char notlpn [20];
	char nama[50];
	char email[50];
	int stok;
	struct node *next;
}*front = NULL, *rear = NULL, *top = NULL;

int nomorantrian = 0;

int countqueue();

int countstack(){
	struct node *temp;
	int count = 0;
	
	temp = top;
	while(temp != 0){
		count++;
		temp = temp->next;
	}
	printf("\n----------------------------");
	printf("\n STOK BUKU SAAT INI : %d \n",count);
	printf("----------------------------\n");
	return count;
}
